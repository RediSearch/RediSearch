      {
        "name": "option",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.SEARCH": {
    "summary": "Searches the index with a textual query, returning either documents or just ids",
    "complexity": "O(N)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      },
      {
        "name": "nocontent",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "NOCONTENT"
        ]
      },
      {
        "name": "verbatim",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "VERBATIM"
        ]
      },
      {
        "name": "nostopwords",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "NOSTOPWORDS"
        ]
      },
      {
        "name": "withscores",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "WITHSCORES"
        ]
      },
      {
        "name": "withpayloads",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "WITHPAYLOADS"
        ]
      },
      {
        "name": "withsortkeys",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "WITHSORTKEYS"
        ]
      },
      {
        "name": "filter",
        "type": "block",
        "optional": true,
        "multiple": true,
        "arguments": [
          {
            "name": "numeric_field",
            "type": "string",
            "command":"FILTER"
          },
          {
            "name": "min",
            "type": "double"
          },
          {
            "name": "max",
            "type": "double"
          }
        ]
      },
      {
        "name": "geo_filter",
        "type": "block",
        "optional": true,
        "multiple": true,
        "arguments": [
          {
            "name": "geo_field",
            "type": "string",
            "command":"GEOFILTER"
          },
          {
            "name": "lon",
            "type": "double"
          },
          {
            "name": "lat",
            "type": "double"
          },
          {
            "name": "radius",
            "type": "double"
          },
          {
            "name": "radius_type",
            "type": "oneof",
            "arguments": [
              "m", "km", "mi", "ft"
            ]
          }
        ]
      },
      {
        "name": "in_keys",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "count",
            "type": "string",
            "command":"INKEYS"
          },
          {
            "name": "key",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "in_fields",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "count",
            "type": "string",
            "command":"INFIELDS"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "return",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "count",
            "type": "string",
            "command":"RETURN"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "summarize",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "summarize",
            "type": "oneof",
            "arguments": [
              "SUMMARIZE"
            ]
          },
          {
            "name": "fields",
            "type": "block",
            "optional": true,
            "arguments": [
              {
                "name": "count",
                "type": "string",
                "command":"FIELDS"
              },
              {
                "name": "field",
                "type": "string",
                "multiple": true
              }
            ]
          },
          {
            "name": "num",
            "type": "integer",
            "command":"FRAGS",
            "optional": true
          },
          {
            "name": "fragsize",
            "type": "integer",
            "command":"LEN",
            "optional": true
          },
          {
            "name": "separator",
            "type": "string",
            "command":"SEPARATOR",
            "optional": true
          }
        ]
      },
      {
        "name": "highlight",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "highlight",
            "type": "oneof",
            "arguments": [
              "HIGHLIGHT"
            ]
          },
          {
            "name": "fields",
            "type": "block",
            "optional": true,
            "arguments": [
              {
                "name": "count",
                "type": "string",
                "command":"FIELDS"
              },
              {
                "name": "field",
                "type": "string",
                "multiple": true
              }
            ]
          },
          {
            "name": "tags",
            "type": "block",
            "optional": true,
            "arguments": [
              {
                "name": "tags",
                "type": "oneof",
                "arguments": [
                  "TAGS"
                ]
              },
              {
                "name": ["open", "close"],
                "type": ["string", "string"]
              }
            ]
          }
        ]
      },
      {
        "name": "slop",
        "type": "integer",
        "optional": true,
        "command":"SLOP"
      },
      {
        "name": "timeout",
        "type": "integer",
        "optional": true,
        "command":"TIMEOUT"
      },
      {
        "name": "tags",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "INORDER"
        ]
      },
      {
        "name": "language",
        "type": "string",
        "optional": true,
        "command":"LANGUAGE"
      },
      {
        "name": "expander",
        "type": "string",
        "optional": true,
        "command":"EXPANDER"
      },
      {
        "name": "scorer",
        "type": "string",
        "optional": true,
        "command":"SCORER"
      },
      {
        "name": "explainscore",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "EXPLAINSCORE"
        ]
      },
      {
        "name": "payload",
        "type": "string",
        "optional": true,
        "command":"PAYLOAD"
      },
      {
        "name": "sortby",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "sortby",
            "type": "string",
            "command":"SORTBY"
          },
          {
            "name": "order",
            "type": "oneof",
            "optional": true,
            "arguments": [
              "ASC", "DESC"
            ]
          }
        ]
      },
      {
        "name": "limit",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "limit",
            "type": "oneof",
            "arguments": [
              "LIMIT"
            ]
          },
          {
            "name": ["offset", "num"],
            "type": ["integer", "integer"]
          }
        ]
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },

  "FT.AGGREGATE": {
    "summary": "Adds terms to a dictionary",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      },
      {
        "name": "verbatim",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "VERBATIM"
        ]
      },
      {
        "name": "load",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "count",
            "type": "string",
            "command":"LOAD"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "timeout",
        "type": "integer",
        "optional": true,
        "command":"TIMEOUT"
      },
      {
        "name": "loadall",
        "type": "oneof",
        "optional": true,
        "arguments": [
          "LOAD *"
        ]
      },
      {
        "name": "groupby",
        "type": "block",
        "optional": true,
        "multiple": true,
        "arguments": [
          {
            "name": "nargs",
            "type": "integer",
            "command":"GROUPBY"
          },
          {
            "name": "property",
            "type": "string",
            "multiple": true
          },
          {
            "name": "reduce",
            "type": "block",
            "optional": true,
            "multiple": true,
            "arguments": [
              {
                "name": "function",
                "type": "string",
                "command":"REDUCE"
              },
              {
                "name": "nargs",
                "type": "integer"
              },
              {
                "name": "arg",
                "type": "string",
                "multiple": true
              },
              {
                "name": "name",
                "type": "string",
                "command":"AS",
                "optional": true
              }
            ]
          }
        ]
      },
      {
        "name": "sortby",
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "nargs",
            "type": "integer",
            "command":"SORTBY"
