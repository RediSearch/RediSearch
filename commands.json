{
  "FT.CREATE": {
    "summary": "Creates an index with the given spec",
    "complexity": [
      "O(K) at creation where K is the number of fields,",
      "O(N) if scanning the keyspace is triggered, where N is the number of keys in the keyspace"],
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "data_type",
        "command": "ON",
        "type": "oneof",
        "arguments": [
          "HASH", "JSON"
        ],
        "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "arguments": [
          {
            "name": "count",
            "type": "integer",
            "command":"PREFIX"
          },
          {
            "name": "prefix",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "default_lang",
        "type": "string",
        "command":"LANGUAGE",
        "optional": true
      },
      {
        "name": "lang_attribute",
        "type": "string",
        "command":"LANGUAGE_FIELD",
        "optional": true
      },
      {
        "name": "default_score",
        "type": "double",
        "command":"SCORE",
        "optional": true
      },
      {
        "name": "score_attribute",
        "type": "string",
        "command":"SCORE_FIELD",
        "optional": true
      },
      {
        "name": "payload_attribute",
        "type": "string",
        "command":"PAYLOAD_FIELD",
        "optional": true
      },
      {
          "name": "maxtextfields",
          "type": "oneof",
          "arguments": [
              "MAXTEXTFIELDS"
          ],
          "optional": true
      },
      {
        "name": "seconds",
        "type": "double",
        "command":"TEMPORARY",
        "optional": true
      },
      {
        "name": "nooffsets",
        "type": "oneof",
        "arguments": [
            "NOOFFSETS"
        ],
        "optional": true
      },
      {
        "name": "nohl",
        "type": "oneof",
        "arguments": [
            "NOHL"
        ],
        "optional": true
      },
      {
        "name": "nofields",
        "type": "oneof",
        "arguments": [
            "NOFIELDS"
        ],
        "optional": true
      },
      {
        "name": "nofreqs",
        "type": "oneof",
        "arguments": [
            "NOFREQS"
        ],
        "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "command":"STOPWORDS",
        "arguments": [
          {
            "name": "count",
            "type": "integer"
          },
          {
            "multiple": true,
            "name": "stopword",
            "type": "string"
          }
        ]
      },
      {
        "name": "schema",
        "type": "oneof",
        "arguments": [
            "SCHEMA"
        ]
      },
      {
        "name": "field",
        "type": "block",
        "multiple": true,
        "arguments": [
          {
            "name": "field_name",
            "type": "string"
          },
          {
            "name": "alias",
            "type": "string",
            "command": "AS",
            "optional": true
          },
          {
            "name": "field_type",
            "type": "oneof",
            "arguments": [
              "TEXT", "TAG", "NUMERIC", "GEO"
            ]
          },
          {
            "name": "sortable",
            "type": "block",
            "optional": true,
            "arguments": [
              {
                "name": "sortable",
                "type": "oneof",
                "arguments": [ 
                  "SORTABLE"
                ]
              },
              {
                "name": "UNF",
                "type": "oneof",
                "arguments": [ 
                  "UNF"
                ],
                "optional": true
              }
            ]
          },
          {
            "name": "noindex",
            "type": "oneof",
            "optional": true,
            "arguments": [
              "NOINDEX"
            ]
          }
        ]
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.INFO": {
    "summary": "Returns information and statistics on the index",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.EXPLAIN": {
    "summary": "Returns the execution plan for a complex query",
