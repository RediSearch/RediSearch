      {
        "name": "alias",
        "type": "string"
      },
      {
        "name": "index",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.ALIASDEL": {
    "summary": "Deletes an alias from the index",
    "complexity": "O(1)",
