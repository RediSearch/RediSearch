      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.ALTER": {
    "summary": "Adds a new field to the index",
    "complexity": "O(N) where N is the number of keys in the keyspace",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "schema",
        "type": "oneof",
        "arguments": [
          "SCHEMA"
        ]
      },
      {
        "name": "add",
        "type": "oneof",
        "arguments": [
          "ADD"
        ]
      },
      {
        "name": "field",
        "type": "string"
      },
      {
        "name": "options",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.DROPINDEX": {
    "summary": "Deletes the index",
    "complexity": "O(1) or O(N) if documents are deleted, where N is the number of keys in the keyspace",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "delete docs",
        "type": "oneof",
        "arguments": [
          "DD"
        ],
        "optional": true
      }
    ],
    "since": "2.0.0",
    "group": "search"
  },
  "FT.ALIASADD": {
    "summary": "Adds an alias to the index",
    "complexity": "O(1)",
