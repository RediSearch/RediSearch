{
  "FT.CREATE": {
    "summary": "Creates an index with the given spec",
    "complexity": [
      "O(K) at creation where K is the number of fields,",
      "O(N) if scanning the keyspace is triggered, where N is the number of keys in the keyspace"],
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "data_type",
        "command": "ON",
        "type": "enum",
        "enum": [
          "HASH", "JSON"
        ],
        "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "integer",
            "command":"PREFIX"
          },
          {
            "name": "prefix",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "default_lang",
        "type": "string",
        "command":"LANGUAGE",
        "optional": true
      },
      {
        "name": "lang_attribute",
        "type": "string",
        "command":"LANGUAGE_FIELD",
        "optional": true
      },
      {
        "name": "default_score",
        "type": "double",
        "command":"SCORE",
        "optional": true
      },
      {
        "name": "score_attribute",
        "type": "string",
        "command":"SCORE_FIELD",
        "optional": true
      },
      {
        "name": "payload_attribute",
        "type": "string",
        "command":"PAYLOAD_FIELD",
        "optional": true
      },
      {
          "name": "maxtextfields",
          "type": "enum",
          "enum": [
              "MAXTEXTFIELDS"
          ],
          "optional": true
      },
      {
        "name": "seconds",
        "type": "double",
        "command":"TEMPORARY",
        "optional": true
      },
      {
        "name": "nooffsets",
        "type": "enum",
        "enum": [
            "NOOFFSETS"
        ],
        "optional": true
      },
      {
        "name": "nohl",
        "type": "enum",
        "enum": [
            "NOHL"
        ],
        "optional": true
      },
      {
        "name": "nofields",
        "type": "enum",
        "enum": [
            "NOFIELDS"
        ],
        "optional": true
      },
      {
        "name": "nofreqs",
        "type": "enum",
        "enum": [
            "NOFREQS"
        ],
        "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "command":"STOPWORDS",
        "block": [
          {
            "name": "count",
            "type": "integer"
          },
          {
            "multiple": true,
            "name": "stopword",
            "type": "string"
          }
        ]
      },
      {
        "name": "schema",
        "type": "enum",
        "enum": [
            "SCHEMA"
        ]
      },
      {
        "name": "field",
        "type": "block",
        "multiple": true,
        "block": [
          {
            "type": "block",
            "block": [
              {
                "name": "field_name",
                "type": "string"
              },
              {
                "name": "alias",
                "type": "string",
                "command": "AS",
                "optional": true
              }
            ]
          },
          {
            "name": "field_type",
            "type": "enum",
            "TEXT": [
              {
                "name": "type",
                "type": "string"
              }
            ],
            "GEO": [
              {
                "name": "type",
                "type": "string"
                
              }
            ],
            "enum": [
              "TEXT", "TAG", "NUMERIC", "GEO"
            ]
          },
          {
            "name": "sortable",
            "type": "enum",
            "enum": [ 
              "SORTABLE"
            ],
            "optional":true
          }
        ]
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.INFO": {
    "summary": "Returns information and statistics on the index",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.EXPLAIN": {
    "summary": "Returns the execution plan for a complex query",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.EXPLAINCLI": {
    "summary": "Returns the execution plan for a complex query",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.ALTER": {
    "summary": "Adds a new field to the index",
    "complexity": "O(N) where N is the number of keys in the keyspace",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "schema",
        "type": "enum",
        "enum": [
          "SCHEMA"
        ]
      },
      {
        "name": "add",
        "type": "enum",
        "enum": [
          "ADD"
        ]
      },
      {
        "name": "field",
        "type": "string"
      },
      {
        "name": "options",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.DROPINDEX": {
    "summary": "Deletes the index",
    "complexity": "O(1) or O(N) if documents are deleted, where N is the number of keys in the keyspace",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "delete docs",
        "type": "enum",
        "enum": [
          "DD"
        ],
        "optional": true
      }
    ],
    "since": "2.0.0",
