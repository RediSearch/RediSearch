      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.EXPLAINCLI": {
    "summary": "Returns the execution plan for a complex query",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
