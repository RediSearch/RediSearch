        "type": "string"
      },
      {
        "name": "value",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.CONFIG HELP": {
    "summary": "Help description of runtime configuration options",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "option",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.SEARCH": {
    "summary": "Searches the index with a textual query, returning either documents or just ids",
    "complexity": "O(N)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      },
      {
        "name": "nocontent",
        "type": "enum",
        "optional": true,
        "enum": [
          "NOCONTENT"
        ]
      },
      {
        "name": "verbatim",
        "type": "enum",
        "optional": true,
        "enum": [
          "VERBATIM"
        ]
      },
      {
        "name": "nostopwords",
        "type": "enum",
        "optional": true,
        "enum": [
          "NOSTOPWORDS"
        ]
      },
      {
        "name": "withscores",
        "type": "enum",
        "optional": true,
        "enum": [
          "WITHSCORES"
        ]
      },
      {
        "name": "withpayloads",
        "type": "enum",
        "optional": true,
        "enum": [
          "WITHPAYLOADS"
        ]
      },
      {
        "name": "withsortkeys",
        "type": "enum",
        "optional": true,
        "enum": [
          "WITHSORTKEYS"
        ]
      },
      {
        "name": "withpayloads",
        "type": "enum",
        "optional": true,
        "enum": [
          "WITHPAYLOADS"
        ]
      },
      {
        "name": "filter",
        "type": "block",
        "optional": true,
        "multiple": true,
        "block": [
          {
            "name": "numeric_field",
            "type": "string",
            "command":"FILTER"
          },
          {
            "name": "min",
            "type": "double"
          },
          {
            "name": "max",
            "type": "double"
          }
        ]
      },
      {
        "name": "geo_filter",
        "type": "block",
        "optional": true,
        "multiple": true,
        "block": [
          {
            "name": "geo_field",
            "type": "string",
            "command":"GEOFILTER"
          },
          {
            "name": "lon",
            "type": "double"
          },
          {
            "name": "lat",
            "type": "double"
          },
          {
            "name": "radius",
            "type": "double"
          },
          {
            "name": "radius_type",
            "type": "enum",
            "enum": [
              "m", "km", "mi", "ft"
            ]
          }
        ]
      },
      {
        "name": "in_keys",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "string",
            "command":"INKEYS"
          },
          {
            "name": "key",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "in_fields",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "string",
            "command":"INFIELDS"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "return",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "string",
            "command":"RETURN"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "summarize",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "summarize",
            "type": "enum",
            "enum": [
              "SUMMARIZE"
            ]
          },
          {
            "name": "fields",
            "type": "block",
            "optional": true,
            "block": [
              {
                "name": "count",
                "type": "string",
                "command":"FIELDS"
              },
              {
                "name": "field",
                "type": "string",
                "multiple": true
              }
            ]
          },
          {
            "name": "num",
            "type": "integer",
            "command":"FRAGS",
            "optional": true
          },
          {
            "name": "fragsize",
            "type": "integer",
            "command":"LEN",
            "optional": true
          },
          {
            "name": "separator",
            "type": "string",
            "command":"SEPARATOR",
            "optional": true
          }
        ]
      },
      {
        "name": "highlight",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "highlight",
            "type": "enum",
            "enum": [
              "HIGHLIGHT"
            ]
          },
          {
            "name": "fields",
            "type": "block",
            "optional": true,
            "block": [
              {
                "name": "count",
                "type": "string",
                "command":"FIELDS"
              },
              {
                "name": "field",
                "type": "string",
                "multiple": true
              }
            ]
          },
          {
            "name": "tags",
            "type": "block",
            "optional": true,
            "block": [
              {
                "name": "tags",
                "type": "enum",
                "enum": [
                  "TAGS"
                ]
              },
              {
                "name": ["open", "close"],
                "type": ["string", "string"]
              }
            ]
          }
        ]
      },
      {
        "name": "slop",
        "type": "integer",
        "optional": true,
        "command":"SLOP"
      },
      {
        "name": "tags",
        "type": "enum",
        "optional": true,
        "enum": [
          "INORDER"
        ]
      },
      {
        "name": "language",
        "type": "string",
        "optional": true,
        "command":"LANGUAGE"
      },
      {
        "name": "expander",
        "type": "string",
        "optional": true,
        "command":"EXPANDER"
      },
      {
        "name": "scorer",
        "type": "string",
        "optional": true,
        "command":"SCORER"
      },
      {
        "name": "explainscore",
        "type": "enum",
        "optional": true,
        "enum": [
          "EXPLAINSCORE"
        ]
      },
      {
        "name": "payload",
        "type": "string",
        "optional": true,
        "command":"PAYLOAD"
      },
      {
        "name": "sortby",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "sortby",
            "type": "string",
            "command":"SORTBY"
          },
          {
            "name": "order",
            "type": "enum",
            "optional": true,
            "enum": [
              "ASC", "DESC"
            ]
          }
        ]
      },
      {
        "name": "limit",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "limit",
            "type": "enum",
            "enum": [
              "LIMIT"
            ]
          },
          {
            "name": ["offset", "num"],
            "type": ["string", "string"]
          }
        ]
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },

  "FT.AGGREGATE": {
    "summary": "Adds terms to a dictionary",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "query",
        "type": "string"
      },
      {
        "name": "verbatim",
        "type": "enum",
        "optional": true,
        "enum": [
          "VERBATIM"
        ]
      },
      {
        "name": "load",
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "string",
            "command":"LOAD"
          },
          {
            "name": "field",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "groupby",
        "type": "block",
        "optional": true,
        "multiple": true,
        "block" : [
          {
            "name": "nargs",
            "type": "integer",
            "command":"GROUPBY"
          },
          {
            "name": "property",
            "type": "string",
            "multiple": true
          },
          {
            "name": "reduce",
            "type": "block",
            "optional": true,
            "multiple": true,
            "block" : [
              {
                "name": "function",
                "type": "string",
                "command":"REDUCE"
              },
              {
                "name": "nargs",
                "type": "integer"
              },
              {
                "name": "arg",
                "type": "string",
                "multiple": true
              },
              {
                "name": "name",
                "type": "string",
                "command":"AS",
                "optional": true
              }
            ]
          }
        ]
      },
      {
        "name": "sortby",
        "type": "block",
        "optional": true,
        "block" : [
          {
            "name": "nargs",
            "type": "integer",
            "command":"SORTBY"
          },
          {
            "name": "fields",
            "type": "block",
            "optional": true,
            "multiple": true,
            "block" : [
              {
                "name": "property",
                "type": "string"
              },
              {
                "name": "order",
                "type": "enum",
                "enum": [
                  "ASC", "DESC"
                ]
              }
            ]
          },
          {
            "name": "num",
            "type": "integer",
            "command":"MAX",
            "optional": true
