      {
        "name": "alias",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.TAGVALS": {
    "summary": "Returns the distinct tags indexed in a Tag field",
    "complexity": "O(N)",
    "arguments": [
      {
        "name": "index",
        "type": "string"
      },
      {
        "name": "field_name",
        "type": "string"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.SUGADD": {
    "summary": "Adds a suggestion string to an auto-complete suggestion dictionary",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "key",
        "type": "string"
      },
      {
        "name": "string",
        "type": "string"
      },
      {
        "name": "score",
        "type": "double"
      },
      {
        "name": "increment score",
        "type": "oneof",
        "arguments": [
          "INCR"
        ],
        "optional": true
      },
      {
          "name": "payload",
          "command": "PAYLOAD",
          "type": "string",
          "optional": true
      }
    ],
    "since": "1.0.0",
    "group": "suggestion"
  },
  "FT.SUGGET": {
    "summary": "Gets completion suggestions for a prefix",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "key",
        "type": "string"
      },
      {
        "name": "prefix",
        "type": "string"
      },
      {
        "name": "fuzzy",
        "type": "oneof",
        "arguments": [
          "FUZZY"
        ],
        "optional": true
      },
      {
        "name": "withscores",
        "type": "oneof",
        "arguments": [
          "WITHSCORES"
        ],
        "optional": true
      },
      {
        "name": "withpayloads",
        "type": "oneof",
        "arguments": [
          "WITHPAYLOADS"
        ],
        "optional": true
      },
      {
          "name": "max",
          "command": "MAX",
