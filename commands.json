{
  "FT.CREATE": {
    "summary": "Creates an index with the given spec",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "key"
      },
      {
        "name": "data_type",
        "command": "ON",
        "type": "enum",
        "enum": [
          "HASH", "JSON"
        ],
        "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "count",
            "type": "integer",
            "command":"PREFIX"
          },
          {
            "name": "prefix",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
        "name": "default_lang",
        "type": "string",
        "command":"LANGUAGE"
      },
      {
        "name": "lang_attribute",
        "type": "string",
        "command":"LANGUAGE_FIELD"
      },
      {
        "name": "default_score",
        "type": "double",
        "command":"SCORE"
      },
      {
        "name": "score_attribute",
        "type": "string",
        "command":"SCORE_FIELD"
      },
      {
        "name": "payload_attribute",
        "type": "string",
        "command":"PAYLOAD_FIELD"
      },
      {
          "name": "maxtextfields",
          "type": "enum",
          "enum": [
              "MAXTEXTFIELDS"
          ],
          "optional": true
      },
      {
        "name": "seconds",
        "type": "double",
        "command":"TEMPORARY"
      },
      {
          "name": "nooffsets",
          "type": "enum",
          "enum": [
              "NOOFFSETS"
          ],
          "optional": true
      },
      {
          "name": "nohl",
          "type": "enum",
          "enum": [
              "NOHL"
          ],
          "optional": true
      },
      {
          "name": "nofields",
          "type": "enum",
          "enum": [
              "NOFIELDS"
          ],
          "optional": true
      },
      {
          "name": "nofreqs",
          "type": "enum",
          "enum": [
              "NOFREQS"
          ],
          "optional": true
      },
      {
        "type": "block",
        "optional": true,
        "block": [
          {
            "name": "num",
            "type": "integer",
            "command":"STOPWORDS"
          },
          {
            "name": "stopword",
            "type": "string",
            "multiple": true
          }
        ]
      },
      {
          "name": "schema",
          "type": "enum",
          "enum": [
              "SCHEMA"
          ]
      },
      {
        "type": "block",
        "multiple": true,
        "block": [
          {
            "name": "num",
            "type": "integer",
            "command":"STOPWORDS"
          },
          {
            "name": "stopword",
            "type": "string",
            "multiple": true
          }
        ]
      }
    ],
    "since": "1.0.0",
    "group": "search"
  },
  "FT.DROPINDEX": {
    "summary": "Deletes the index",
    "complexity": "O(N)",
    "arguments": [
      {
        "name": "index",
        "type": "key"
      },
      {
        "name": "deletedocs",
        "type": "enum",
        "enum": [
          "DD"
        ],
        "optional": true
      }
    ],
    "since": "2.0.0",
    "group": "search"
  },
  "FT.INFO": {
    "summary": "Returns information and statistics on the index",
    "complexity": "O(1)",
    "arguments": [
      {
        "name": "index",
        "type": "key"
      }
    ],
    "since": "1.0.0",
    "group": "search"
  }
}
